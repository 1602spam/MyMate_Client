<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="연습.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:연습"
    Shell.FlyoutBehavior="Disabled">

    

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage2}"
        Route="MainPage2" />

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage3}"
        Route="MainPage3" />

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage4}"
        Route="MainPage4" />

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:tabview}"
        Route="tabview" />

</Shell>
